    "def centered_average(nums):\n",
    "  count=0 \n",
    "  max1=max(nums)\n",
    "  min1=min(nums)\n",
    "  if len(nums)>=3:\n",
    "    for i in nums :\n",
    "      count=count+i \n",
    "    count=count-max1\n",
    "    count=count-min1\n",
    "    return count/(len(nums)-2)\n",
    "      "
   ]
  }
